{
  "name": "moga-component-build-plugin",
  "version": "1.0.10",
  "description": "#### Description 基于render-builder封装的编译react组件的build-plugin",
  "main": "lib/index.js",
  "scripts": {
    "copy": "node copy.js",
    "start": "tsc -w",
    "build": "tsc",
    "lint": "eslint --cache --quiet --ext .js,.ts ./",
    "lint-fix": "eslint --cache --ext .js,.ts ./ --fix",
    "prepublishOnly": "npm run build"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/render-ydb/moga-component-build-plugin.git"
  },
  "files": [
    "lib"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.43.0",
    "husky": "^1.3.1",
    "render-builder": "1.0.11",
    "typescript": "^4.9.5",
    "webpack": "^5.87.0",
    "webpack-chain": "^6.5.1"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "glob": "^10.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.5",
    "moga-app-base-webpack-config": "1.0.0",
    "postcss-px-to-viewport": "^1.1.1",
    "react-dev-utils": "^12.0.1"
  }
}
